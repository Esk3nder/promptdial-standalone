{
  "version": 2,
  "builds": [
    {
      "src": "packages/core/src/server.ts",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "packages/core/src/server.ts"
    }
  ],
  "env": {
    "PROMPTDIAL_MODE": "monolithic",
    "NODE_ENV": "production"
  }
}